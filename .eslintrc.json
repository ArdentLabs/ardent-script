{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "ignorePatterns": ["dist/**/*"],
  "rules": {
    "prettier/prettier": "warn",
    "no-useless-rename": "error"
  }
}
