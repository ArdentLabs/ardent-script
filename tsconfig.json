{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "downlevelIteration": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/__tests__/**/*", "**/__mocks__/**/*"]
}
